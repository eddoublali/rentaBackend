{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "ipa/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/ipa/(.*)",
      "dest": "/ipa/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.ts"
    }
  ]
}
